# platform :ios, '9.0'

target 'GeoCaching' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GeoCaching
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'

  target 'GeoCachingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GeoCachingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
